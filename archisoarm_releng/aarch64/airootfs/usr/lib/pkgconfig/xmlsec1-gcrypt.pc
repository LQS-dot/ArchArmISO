prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: xmlsec1-gcrypt
Version: 1.2.37
Description: XML Security Library implements XML Signature and XML Encryption standards
Requires: libxml-2.0 >= 2.8.0 libxslt >= 1.0.20
Cflags:  -D__XMLSEC_FUNCTION__=__func__ -DXMLSEC_NO_SIZE_T -DXMLSEC_NO_GOST=1 -DXMLSEC_NO_GOST2012=1 -DXMLSEC_DL_LIBLTDL=1 -I${prefix}/include/xmlsec1    -DXMLSEC_CRYPTO_GCRYPT=1
Cflags.private: -DXMLSEC_STATIC
Libs: -L${exec_prefix}/lib -lxmlsec1-gcrypt -lxmlsec1 -lltdl  -lgcrypt 
